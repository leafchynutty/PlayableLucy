import funkin.modding.module.Module;

class DebugToolManager extends Module
{
        public var tools = [
                "putmeinfreeplay" => true
        ];

        public function new() {
                super("DebugToolManager");
        }

        public function getToolEnabled(tool:String):Bool
        {
                if (tools.exists(tool))
                        return tools.get(tool);

                return false;
        }
}